{
  "name": "travel-assistant-app",
  "version": "1.0.0",
  "description": "智能旅游助手应用程序",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-deps": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.5.0",
    "node-fetch": "^3.3.2",
    "form-data": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1"
  },
  "keywords": ["travel", "tourism", "ai", "assistant"],
  "author": "Travel Assistant Team",
  "license": "MIT"
}
